{
    "name": "wakup/monge-php-api",
    "description": "PHP interface for Monge web services",
    "version": "0.0.8",
    "type": "library",
    "authors": [
        {
            "name": "Alex Gutiérrez",
            "email": "alejandro.gutierrez@wakup.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "6.3.3",
        "ext-json": "*",
        "netresearch/jsonmapper": "v1.4.0",
        "kamermans/guzzle-oauth2-subscriber": "v1.0.6"
    },
    "autoload": {
        "psr-4": {
            "Wakup\\": ["src/", "src/models/monge/", "src/models/products/","src/communications/"],
            "Wakup\\Requests\\": ["src/communications/requests"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
